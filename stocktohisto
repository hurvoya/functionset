Fait_All =
UNION(
    SELECTCOLUMNS(
        contracts,
        "Country", contracts[Country],
        "IBL", contracts[IBL],
        "Partner", contracts[Partner],
        "Organisme", contracts[Organisme],
        "Financial Product", contracts[Financial Product],
        "Commercial Product", contracts[Commercial Product],
        "Market", contracts[Market],
        "Apporteur", contracts[Apporteur],
        "Agence", contracts[Agence],
        "Commercial", contracts[Commercial]
    ),
    SELECTCOLUMNS(
        requests,
        "Country", requests[Country],
        "IBL", requests[IBL],
        "Partner", requests[Partner],
        "Organisme", requests[Organisme],
        "Financial Product", requests[Financial Product],
        "Commercial Product", requests[Commercial Product],
        "Market", requests[Market],
        "Apporteur", requests[Apporteur],
        "Agence", requests[Agence],
        "Commercial", requests[Commercial]
    ),
    SELECTCOLUMNS(
        clients,
        "Country", clients[Country],
        "IBL", clients[IBL],
        "Partner", clients[Partner],
        "Organisme", clients[Organisme],
        "Financial Product", clients[Financial Product],
        "Commercial Product", clients[Commercial Product],
        "Market", clients[Market],
        "Apporteur", clients[Apporteur],
        "Agence", clients[Agence],
        "Commercial", clients[Commercial]
    )
)
