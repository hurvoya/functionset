Nb_Total_Requests :=
CALCULATE(
    COUNTROWS(requests),
    TREATAS(VALUES(Fait_All[Country]), requests[Country]),
    TREATAS(VALUES(Fait_All[IBL]), requests[IBL]),
    TREATAS(VALUES(Fait_All[Partner]), requests[Partner]),
    TREATAS(VALUES(Fait_All[Organisme]), requests[Organisme]),
    TREATAS(VALUES(Fait_All[Financial Product]), requests[Financial Product]),
    TREATAS(VALUES(Fait_All[Commercial Product]), requests[Commercial Product]),
    TREATAS(VALUES(Fait_All[Market]), requests[Market]),
    TREATAS(VALUES(Fait_All[Apporteur]), requests[Apporteur]),
    TREATAS(VALUES(Fait_All[Agence]), requests[Agence]),
    TREATAS(VALUES(Fait_All[Commercial]), requests[Commercial])
)
