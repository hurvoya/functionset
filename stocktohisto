Dim_Country = DISTINCT(
    UNION(
        SELECTCOLUMNS(contract_request, "Country", contract_request[Country]),
        SELECTCOLUMNS(clients, "Country", clients[Country])
    )
)

Dim_IBL = DISTINCT(
    UNION(
        SELECTCOLUMNS(contract_request, "IBL", contract_request[IBL]),
        SELECTCOLUMNS(clients, "IBL", clients[IBL])
    )
)

