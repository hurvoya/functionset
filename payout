Fait_All =
UNION (
    SELECTCOLUMNS (
        MEL,
        "Country", MEL[Country],
        "Contract Id", MEL[Contract Id],
        "IBL", MEL[IBL],
        "Organisme", MEL[Organisme],
        "Partner", MEL[Partner],
        "Financial Product", MEL[Financial Product],
        "Commercial Product", MEL[Commercial Product],
        "Market", MEL[Market],
        "Apporteur", MEL[Apporteur],
        "Agence", MEL[Agence]
    ),
    SELECTCOLUMNS (
        Cancel_STL,
        "Country", Cancel_STL[Country],
        "Contract Id", Cancel_STL[Contract Id],
        "IBL", Cancel_STL[IBL],
        "Organisme", Cancel_STL[Organisme],
        "Partner", Cancel_STL[Partner],
        "Financial Product", Cancel_STL[Financial Product],
        "Commercial Product", Cancel_STL[Commercial Product],
        "Market", Cancel_STL[Market],
        "Apporteur", Cancel_STL[Apporteur],
        "Agence", Cancel_STL[Agence]
    ),
    SELECTCOLUMNS (
        Spotfire,
        "Country", Spotfire[Country],
        "Contract Id", Spotfire[Contract Id],
        "IBL", Spotfire[IBL],
        "Organisme", Spotfire[Organisme],
        "Partner", Spotfire[Partner],
        "Financial Product", Spotfire[Financial Product],
        "Commercial Product", Spotfire[Commercial Product],
        "Market", Spotfire[Market],
        "Apporteur", Spotfire[Apporteur],
        "Agence", Spotfire[Agence]
    )
)
