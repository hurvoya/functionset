def touch_category(nb_touch):
    if nb_touch < 1:
        return "Touch 0"
    elif nb_touch == 1:
        return "Touch 1"
    elif nb_touch == 2:
        return "Touch 2"
    elif nb_touch == 3:
        return "Touch 3"
    else:
        return "Touch 4+"

# Application sur le dataframe
df['Touch_Type'] = df['Nb of touch (total)'].apply(touch_category)
