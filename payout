Total_WorkingDays :=
CALCULATE(
    SUM(FTE[WorkingDays]),
    TREATAS(VALUES(Fait_All[Country]), FTE[Country]),
    TREATAS(VALUES(Fait_All[IBL]), FTE[IBL])
)


Total_Actual_FTE :=
CALCULATE(
    SUM(FTE[Actual FTE]),
    TREATAS(VALUES(Fait_All[Country]), FTE[Country]),
    TREATAS(VALUES(Fait_All[IBL]), FTE[IBL])
)


Total_Op_FTE :=
CALCULATE(
    SUM(FTE[Op FTE]),
    TREATAS(VALUES(Fait_All[Country]), FTE[Country]),
    TREATAS(VALUES(Fait_All[IBL]), FTE[IBL])
)


Total_TaskForce_FTE :=
CALCULATE(
    SUM(FTE[Task Force FTE]),
    TREATAS(VALUES(Fait_All[Country]), FTE[Country]),
    TREATAS(VALUES(Fait_All[IBL]), FTE[IBL])
)
