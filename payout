def lead_time_category(leadtime):
    if leadtime < 0.5001:
        return "< 4hours"
    elif leadtime < 1.0001:
        return "< 1day"
    elif leadtime < 2.0001:
        return "< 2days"
    elif leadtime < 3.0001:
        return "< 3days"
    elif leadtime < 5.0001:
        return "< 5days"
    else:
        return "> 5days"

# Appliquer la fonction
df['LeadTime_Type'] = df['Last Doc LeadTime in days'].apply(lead_time_category)
