df_siren_clean = (
    df[["col-0"]]                    # on garde uniquement la colonne
    .rename(columns={"col-0": "siren"})
    .dropna(subset=["siren"])        # enlève tous les NaN
    .drop_duplicates()               # enlève les doublons
    .reset_index(drop=True)          # index propre
)
