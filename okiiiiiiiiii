# GLOBAL
plot_bloc2_typologie(kpi_bloc2_global, title_suffix="Global (ELS + TLS)")

# ELS
kpi_bloc2_els = kpi_bloc2_ibl[kpi_bloc2_ibl["ibl"] == "ELS"].copy()
kpi_bloc2_els = kpi_bloc2_els.drop(columns=["ibl"])
plot_bloc2_typologie(kpi_bloc2_els, title_suffix="IBL = ELS")

# TLS
kpi_bloc2_tls = kpi_bloc2_ibl[kpi_bloc2_ibl["ibl"] == "TLS"].copy()
kpi_bloc2_tls = kpi_bloc2_tls.drop(columns=["ibl"])
plot_bloc2_typologie(kpi_bloc2_tls, title_suffix="IBL = TLS")
