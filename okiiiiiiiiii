kpi_bloc6_delay_ibl = (
    agg_delay
    .groupby(["year", "country", "ibl"], as_index=False)
    .agg(
        mean_delay_days=("delay_days", "mean"),
        median_delay_days=("delay_days", "median"),
        nb_dealers_8=("id_apporteur", "nunique"),
    )
)

kpi_bloc6_delay_ibl["mean_delay_days"] = kpi_bloc6_delay_ibl["mean_delay_days"].round(1)
kpi_bloc6_delay_ibl["median_delay_days"] = kpi_bloc6_delay_ibl["median_delay_days"].round(1)
