# Global (ELS + TLS)
plot_bloc6_delay(kpi_bloc6_delay_global, "Global (ELS + TLS)")

# ELS
kpi_bloc6_delay_els = kpi_bloc6_delay_ibl[kpi_bloc6_delay_ibl["ibl"] == "ELS"].drop(columns=["ibl"])
plot_bloc6_delay(kpi_bloc6_delay_els, "IBL = ELS")

# TLS
kpi_bloc6_delay_tls = kpi_bloc6_delay_ibl[kpi_bloc6_delay_ibl["ibl"] == "TLS"].drop(columns=["ibl"])
plot_bloc6_delay(kpi_bloc6_delay_tls, "IBL = TLS")
