# Global (ELS + TLS)
plot_bloc6_rate(kpi_bloc6_rate_global, "Global (ELS + TLS)")

# ELS seulement
plot_bloc6_rate(
    kpi_bloc6_rate_ibl[kpi_bloc6_rate_ibl["ibl"] == "ELS"].drop(columns=["ibl"]),
    "IBL = ELS"
)

# TLS seulement
plot_bloc6_rate(
    kpi_bloc6_rate_ibl[kpi_bloc6_rate_ibl["ibl"] == "TLS"].drop(columns=["ibl"]),
    "IBL = TLS"
)

    ax.set_ylim(0, max_val * 1.20 if max_val > 0 else 5)

    plt.tight_layout()
    plt.show()
