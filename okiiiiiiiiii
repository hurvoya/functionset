# Global
plot_bloc1_volume(
    kpi_bloc1_global,
    title_suffix="Global (ELS + TLS)",
)

# ELS
kpi_bloc1_els = kpi_bloc1_ibl[kpi_bloc1_ibl["ibl"] == "ELS"].copy()
plot_bloc1_volume(
    kpi_bloc1_els.drop(columns=["ibl"]),
    title_suffix="IBL = ELS",
)

# TLS
kpi_bloc1_tls = kpi_bloc1_ibl[kpi_bloc1_ibl["ibl"] == "TLS"].copy()
plot_bloc1_volume(
    kpi_bloc1_tls.drop(columns=["ibl"]),
    title_suffix="IBL = TLS",
)

