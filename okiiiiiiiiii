def build_final_table_v2(follow):
    """
    follow : issu de build_follow_v2

    Retour :
    final : table prête à être envoyée aux pays (avant filtres année/KA)
    """

    final = follow.copy()

    final = final[
        [
            "country",
            "ibl",
            "id_apporteur",
            "new_dealer_type",
            "dte_creation_apporteur",
            "dte_new_dealer",
            "nb_calls_total",
            "nb_contrats_total",
            "is_key_account",
            "en_suivi",
            "sorti_suivi",
            "event_id",
        ]
    ]

    return final


    return follow
