for year in [2024, 2025]:
    # GLOBAL
    plot_pct_calls_par_pays(impact_seuil_country, year=year, ibl_scope="GLOBAL")
    plot_nb_calls_par_pays(impact_seuil_country, year=year, ibl_scope="GLOBAL")

    # ELS
    plot_pct_calls_par_pays(impact_seuil_country, year=year, ibl_scope="ELS")
    plot_nb_calls_par_pays(impact_seuil_country, year=year, ibl_scope="ELS")

    # TLS
    plot_pct_calls_par_pays(impact_seuil_country, year=year, ibl_scope="TLS")
    plot_nb_calls_par_pays(impact_seuil_country, year=year, ibl_scope="TLS")
