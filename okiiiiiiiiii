# 1) Global (ELS + TLS)
plot_bloc5_delay(kpi_bloc5_global, title_suffix="Global (ELS + TLS)")

# 2) ELS
kpi_bloc5_els = kpi_bloc5_ibl[kpi_bloc5_ibl["ibl"] == "ELS"].drop(columns=["ibl"])
plot_bloc5_delay(kpi_bloc5_els, title_suffix="IBL = ELS")

# 3) TLS
kpi_bloc5_tls = kpi_bloc5_ibl[kpi_bloc5_ibl["ibl"] == "TLS"].drop(columns=["ibl"])
plot_bloc5_delay(kpi_bloc5_tls, title_suffix="IBL = TLS")
