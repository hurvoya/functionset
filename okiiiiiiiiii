df6 = calls.copy()

# Exclure Key Accounts
df6 = df6[~df6["is_key_account"]].copy()

# Garder uniquement les calls valides
df6 = df6[df6["dte_mel"].notna() & df6["dte_new_dealer"].notna()].copy()

# Année de détection
df6["year"] = df6["dte_new_dealer"].dt.year

# Focus sur 2024–2025
df6 = df6[df6["year"].isin([2024, 2025])].copy()
