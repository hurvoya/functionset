# --- GLOBAL ---
plot_calls_total(kpi_calls_total, "Global")
plot_calls_mean(kpi_calls_mean, "Global")

# --- ELS ---
plot_calls_total(kpi_calls_total[kpi_calls_total["ibl"]=="ELS"], "IBL = ELS")
plot_calls_mean(kpi_calls_mean[kpi_calls_mean["ibl"]=="ELS"], "IBL = ELS")

# --- TLS ---
plot_calls_total(kpi_calls_total[kpi_calls_total["ibl"]=="TLS"], "IBL = TLS")
plot_calls_mean(kpi_calls_mean[kpi_calls_mean["ibl"]=="TLS"], "IBL = TLS")
