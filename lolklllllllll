import pandas as pd
import numpy as np

def month_end(s: pd.Series) -> pd.Series:
    s = pd.to_datetime(s, errors="coerce")
    return s.dt.to_period("M").dt.to_timestamp("M")

def add_months(dt: pd.Series, n: int) -> pd.Series:
    # dt datetime64, retourne dt + n mois (calendaire)
    return dt + pd.DateOffset(months=n)
