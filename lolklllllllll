# GLOBAL
kpi_global = compute_kpi_volume(df)
plot_volume_key_vs_nonkey(
    kpi_global,
    "Existing dealers identified (Growth > 30%) – Key vs Non-Key (2025)"
)

# ELS
kpi_els = compute_kpi_volume(df, ibl="ELS")
plot_volume_key_vs_nonkey(
    kpi_els,
    "Existing dealers identified – ELS – Key vs Non-Key (2025)"
)

# TLS
kpi_tls = compute_kpi_volume(df, ibl="TLS")
plot_volume_key_vs_nonkey(
    kpi_tls,
    "Existing dealers identified – TLS – Key vs Non-Key (2025)"
)
