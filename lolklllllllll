kpi_block1_els = (
    df[df["ibl"] == "ELS"]
    .groupby(["country", "year", "is_key_account"], as_index=False)
    .agg(n_dealers=("id_apporteur", "nunique"))
)
kpi_block1_tls = (
    df[df["ibl"] == "TLS"]
    .groupby(["country", "year", "is_key_account"], as_index=False)
    .agg(n_dealers=("id_apporteur", "nunique"))
)
