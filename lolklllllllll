# Global
kpi_g = compute_kpi_block2_unique_vs_repeater(events_existing, ibl=None)
plot_block2_unique_vs_repeater(
    kpi_g,
    "Unique vs Repeater Existing Dealers (Non Key Accounts) – 2025 (Global)"
)

# ELS
kpi_els = compute_kpi_block2_unique_vs_repeater(events_existing, ibl="ELS")
plot_block2_unique_vs_repeater(
    kpi_els,
    "Unique vs Repeater Existing Dealers (Non Key Accounts) – 2025 (ELS)"
)

